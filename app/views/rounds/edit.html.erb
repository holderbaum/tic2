<h1>Editing round</h1>

<% form_for(@round) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :player %><br />
    <%= f.text_field :player %>
  </p>
  <p>
    <%= f.label :running %><br />
    <%= f.check_box :running %>
  </p>
  <p>
    <%= f.label :won %><br />
    <%= f.check_box :won %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @round %> |
<%= link_to 'Back', rounds_path %>